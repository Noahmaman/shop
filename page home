    <script>
      $(document).ready(function () {
        $('.ajax-addcartbtn').on('click', function (e) {
          e.preventDefault();
          var qty = 1;
          var $this = $(this);
          $this.addClass('loading');

          var var_id = $(this).attr('data-id');

          fetch('/cart/add.js', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
            },
            body: JSON.stringify({
              quantity: qty,
              id: var_id,
              sections: ['cart-drawer', 'cart-icon-bubble'],
              sections_url: window.location.pathname,
            }),
          })
            .then((response) => {
              if (!response.ok) {
                throw new Error('Network response was not ok');
              }
              return response.json();
            })
            .then((data) => {
              // Success callback - popup cart
              // alert('Product has been added to your cart.');

              fetch(window.location.pathname, {
                method: 'GET', // or 'POST' if your server expects POST
                headers: {
                  'Content-Type': 'text/html', // Adjust the content type if needed
                },
              })
                .then((response) => {
                  if (!response.ok) {
                    throw new Error('Network response was not ok');
                  }
                  return response.text();
                })
                .then((html) => {
                  // Find the new CartDrawer in the response HTML
                  var newCartDrawer = $(html).find('#CartDrawer');
                  var cartbtnicon = $(html).find('#cart-icon-bubble');

                  // Update the current CartDrawer with the new HTML
                  var cartDrawerElement = $('#CartDrawer');
                  if (cartDrawerElement.length && newCartDrawer.length) {
                    cartDrawerElement.html(newCartDrawer.html());
                    $('#cart-icon-bubble').html(cartbtnicon.html());
                    $('cart-drawer').removeClass('is-empty');
                    $('cart-drawer').addClass('animate');
                    $('cart-drawer').addClass('active');
                    $(document).on('click', '#CartDrawer-Overlay', function (e) {
                      e.preventDefault();
                      $('cart-drawer').removeClass('animate');
                      $('cart-drawer').removeClass('active');
                    });

                    //free delivery text change
                    if ($('free-shipping-indicator').length > 1) {
                      // Find all elements with the class 'free-shipping-indicator' within 'CartDrawer'
                      cartDrawerElement.find('free-shipping-indicator').each(function () {
                        // Find child elements with the class 'free-shipping'
                        var stillmoreElement = $(this).find('.still-more');
                        var freeShippingElement = $(this).find('.free-shipping');

                        // Check if '.free-shipping' has class '.hidden'
                        if (freeShippingElement.hasClass('hidden')) {
                          // If it has class '.hidden', add class '.hidden' to child element '.free-shipping'
                          $('free-shipping-indicator .free-shipping').addClass('hidden');

                          // Remove class '.hidden' from child element '.still-more'
                          $('free-shipping-indicator .still-more').removeClass('hidden');
                          $('free-shipping-indicator .still-more').html(stillmoreElement.html());
                        } else {
                          $('free-shipping-indicator .free-shipping').removeClass('hidden');

                          // Remove class '.hidden' from child element '.still-more'
                          $('free-shipping-indicator .still-more').addClass('hidden');
                          $('free-shipping-indicator .still-more').html(stillmoreElement.html());
                        }
                      });
                    }
                  } else {
                    console.error('Element with ID "CartDrawer" not found.');
                  }
                  $this.removeClass('loading');
                })
                .catch((error) => {
                  // Error callback for the second AJAX request
                  alert('ERROR: ' + error.message);
                });
            })
            .catch((error) => {
              // Error callback
              alert('ERROR: ' + error.message);
            });
        });
      });

      jQuery(document).ready(function () {
        jQuery(document).on('click', '#PopupModal-modal', function () {
          jQuery('video').each(function () {
            // this.hasAttribute("autoplay")&&this.play()
            this.pause();
            this.removeAttribute('src'); // empty source
            this.load();
          });
        });
      });

      jQuery(document).ready(function () {
        jQuery(document).on('click', '#ModalClose-waitlist', function () {
          jQuery('video').each(function () {
            //this.hasAttribute("autoplay")&&this.play()
            this.pause();
            this.removeAttribute('src'); // empty source
            this.load();
          });
        });
      });
    </script>
